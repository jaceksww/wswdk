<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WM\WSBundle\Entity\HaWsRating" table="ha_ws_rating">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userid" type="integer" column="userID" nullable="false"/>
    <field name="wwwid" type="integer" column="wwwID" nullable="false"/>
    <field name="rate" type="integer" column="rate" nullable="false"/>
    <field name="datecreated" type="datetime" column="dateCreated" nullable="false"/>
  </entity>
</doctrine-mapping>
